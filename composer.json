    "keywords": ["doctrine", "console", "helper", "object", "entity", "command", "manipulation"],
    "homepage": "https://github.com/sonatra/sonatra-doctrine-console",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "symfony/console": "^3.3.0",
        "doctrine/common": "^2.6.0"
    },
    "require-dev": {
