    "keywords": ["doctrine", "console", "helper", "object", "entity", "command", "manipulation"],
    "homepage": "https://github.com/fxpio/fxp-doctrine-console",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
